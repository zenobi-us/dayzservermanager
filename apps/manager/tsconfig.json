{
  "extends": "./tsconfig.options.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2022",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "declaration": false,
    "types": [
      "vinxi"
    ],
    "paths": {
      ":*": [
        "./app/*"
      ],
      "@dayzserver/sdk": [
        "../../pkgs/sdk/src/index.ts"
      ],
      "@dayzserver/sdk/*": [
        "../../pkgs/sdk/src/*"
      ]
    },
    "outDir": "../../.moon/cache/types/apps/manager"
  },
  "include": [
    "../../pkgs/sdk/**/*",
    "../../types/**/*",
    "**/*.ts",
    "**/*.tsx"
  ],
  "references": [
    {
      "path": "../../pkgs/sdk"
    }
  ]
}
